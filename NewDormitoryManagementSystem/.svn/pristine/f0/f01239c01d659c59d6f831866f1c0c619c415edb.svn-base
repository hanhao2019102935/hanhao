@CHARSET "UTF-8";
@media screen and (min-width: 0px) and (max-width: 1400px) {
				html{
					font-size: 55%;
				}		
			}
			@media screen and (min-width: 1400px) and (max-width: 2000px) {
				html{
					font-size: 62.5%;
				}		
			}
			body{
				margin: 0px;
				padding: 0px;
				background-color: #000C2F;
			}
			#bodyDiv {
				padding-top: 10%;
				height: 80%;
			}
			
			.firstGuid button{
				width: 100%;
				line-height: 300%;
				background-color: #000C2F;
				color: #8C8C8C;
				border-style: none;
				font-size: 1.5rem;
				font-family: "微软雅黑";
				text-align: center;
				outline-style: none;
			}
			
			.firstGuid button:hover{
				width: 100%;
				line-height: 300%;
				background-color: #010011;
				color: white;
				border-style: none;
				font-size: 1.5rem;
				font-family: "微软雅黑";
				text-align: center;
				outline-style: none;
			}
			
			ul{
			 	width: 95%;
				margin: 0px;
				padding: 0px;
				display: none;		
			}
			ul li{
				width: 95%;
				padding-left: 10%;
				line-height: 230%;
				background-color: #337AB7;
				list-style: none;
				font-size: 1.35rem;
				color: white;
				font-family: "微软雅黑";
				
			}
			ul li:hover{
				width: 95%;
				padding-left: 10%;
				line-height: 230%;
				background-color: white;
				list-style: none;
				font-size: 1.35rem;
				color: #337AB7;
				font-family: "微软雅黑";
				
			}
			a{
				text-decoration: none;
				color: white;
			}
			a:hover{
				text-decoration: none;
				color: #337AB7;
			}